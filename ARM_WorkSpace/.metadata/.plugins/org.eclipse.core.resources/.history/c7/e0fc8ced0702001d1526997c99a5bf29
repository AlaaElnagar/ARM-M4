#ifndef MFPEC_PRIVAITE_H_
#define MFPEC_PRIVAITE_H_


typedef struct{

	volatile u32 ACR      ;
	volatile u32 KEYR     ;
	volatile u32 OPTKEYR  ;
	volatile u32 SR       ;
	volatile u32 CR       ;
	volatile u32 AR       ;
	volatile u32 RESERVED ;
	volatile u32 OPR      ;
	volatile u32 WRPR     ;

}FPEC_Type;

#define MFPEC  ((volatile FPEC_Type*)0x40022000)


#endif /* MFPEC_PRIVAITE_H_ */
