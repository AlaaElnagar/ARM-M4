#ifndef MFPEC_PRIVATE_H_
#define MFPEC_PRIVATE_H_

/*******************************************************************************************************/
/*                                    Macros Base Address Of NVIC Registers                            */
/*******************************************************************************************************/

#define           FPEC_BASE_ADRESS         0x40023C00


/*******************************************************************************************************/
/*                                       Register Definitions                                          */
/*-----------------------------------------------------------------------------------------------------*/
/*     - Developer can't Edit in it                                                                    */
/*     - Register _ Defination		                                                                   */
/*     - Design :                                                                                      */
/*    				- #define	:	NO                                                                 */
/*    				- Union		:	NO                                                                 */
/*    				- Struct	:	YES                                                                */
/*                                                                                                     */
/*******************************************************************************************************/

typedef struct{

	volatile u32 ACR      ;
	volatile u32 KEYR     ;
	volatile u32 OPTKEYR  ;
	volatile u32 SR       ;
	volatile u32 CR       ;
	volatile u32 OPTCR    ;

}FPEC_Type;

#define MFPEC  ((volatile FPEC_Type*) FPEC_BASE_ADRESS)


#endif /* MFPEC_PRIVATE_H_ */
